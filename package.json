{
  "name": "vite-browser",
  "author": "Brian Jenkins",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npx tsx scripts/build.ts",
    "dev": "CODE=0 ; (npx vite preview --open & { npx vite build --watch ; CODE=$? ; kill -INT $(jobs -p) ; }) ; (exit $CODE)",
    "rollup": "npx rollup -c",
    "postinstall": "chmod 755 scripts/install.sh && ./scripts/install.sh"
  },
